import java.util.*;
class temp
{
     public static void main(String args[])
     {   
         Scanner sc = new Scanner(System.in);
         float c;
         System.out.println("Enter Fahrenheit Temperature:");
         float f = sc.nextFloat();
         c = (f-32)*5/9;
         System.out.println("celcius Temperature is :"+c);
     }
}
